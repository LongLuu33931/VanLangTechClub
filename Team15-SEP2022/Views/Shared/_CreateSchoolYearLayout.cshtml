@model Team15_SEP2022.Models.SchoolYear


@using (Ajax.BeginForm("CreateSchoolYear", "Admin", null, new AjaxOptions { HttpMethod="POST", Confirm = "Bạn có chắc muốn thêm niêm khóa này?", UpdateTargetId = "upF", OnSuccess = "successCreateSchoolYear", LoadingElementId="loading" }))
{
    <div class="modal fade" id="BTNcreateschoolYear" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Thêm Niên Khóa</h5>
                </div>
                <div id="upF" class="modal-body row">
                    @Html.Partial("_CreateSchoolYearForm", Model)
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary-v2">Tạo</button>
                    <button type="button" class="btn btn-cancel" data-dismiss="modal">Quay về</button>
                </div>
            </div>
        </div>
    </div>
}